{
  "name": "rss-proxy",
  "version": "1.0.5",
  "description": "Simple RSS proxy server with Express and CORS",
  "main": "dist/presentation/server.js",
  "type": "module",
  "engines": {
    "node": ">=18.19.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/presentation/server.js",
    "dev": "tsx watch src/presentation/server.ts | pino-pretty",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "dependencies": {
    "@types/cheerio": "^1.0.0",
    "better-sqlite3": "^11.0.0",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "fast-xml-parser": "^5.3.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "pino": "^10.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/better-sqlite3": "^7.6.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.7.1",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
